# Example Environment Configuration for cost-cron
# Copy this file to .env.dev, .env.uat, or .env.prd and update with your values

# GCP Project
GCP_PROJECT_ID=your-project-id

# BigQuery Configuration
BQ_DATASET_ID=billing_data
BQ_TABLE_ID=daily_costs
BQ_LOCATION=US

# Billing Export Configuration (if using billing export)
BILLING_EXPORT_DATASET=billing_data
BILLING_EXPORT_TABLE_PREFIX=gcp_billing_export

# Logging
LOG_LEVEL=INFO
