# Development Environment Configuration for deployment-pipeline

# Bitbucket Configuration
BITBUCKET_BASE_URL=https://bitbucket.org/your-org
BITBUCKET_USERNAME=your-username
BITBUCKET_APP_PASSWORD=your-app-password
# Branch to fetch versions from (e.g., uat, main, develop)
SOURCE_BRANCH=uat

# Services Configuration
# Path to YAML file containing services list
SERVICES_CONFIG_FILE=services_config.yaml

# Output Configuration
OUTPUT_FILE=services.yaml
# Keep history of previous versions
KEEP_HISTORY=True
HISTORY_FILE=services_history.yaml

# Logging
LOG_LEVEL=INFO
